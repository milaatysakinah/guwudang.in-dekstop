<basic1:MyPage x:Class="guwudang.Sidebar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:guwudang"
        xmlns:basic1="clr-namespace:Velacro.UIElements.Basic;assembly=Velacro"
        mc:Ignorable="d"
        Title="SidebarWindow" Height="450" Width="800">
    <Grid Margin="0,0,10,0">
        <Frame Name="secondFrame" NavigationUIVisibility="Hidden" Margin="80,10,10,10" Height="430" Width="710"/>
        <Button Name="dashboard_btn" Content="Dashboard" HorizontalAlignment="Left" Margin="0,10,0,0" VerticalAlignment="Top" Width="75" Click="dashboard_btn_Click" Grid.Column="1" Height="20" />
        <Button Name="product_btn" Content="Product" HorizontalAlignment="Left" Margin="0,35,0,0" VerticalAlignment="Top" Width="75" Click="product_btn_Click" Grid.Column="1" Height="20" />
        <Button Name="partner_btn" Content="Partner" HorizontalAlignment="Left" Margin="0,60,0,0" VerticalAlignment="Top" Width="75" Click="partner_btn_Click" Grid.Column="1" Height="20" />
        <Button Name="invoice_btn" Content="Invoice" HorizontalAlignment="Left" Margin="0,85,0,0" VerticalAlignment="Top" Width="75" Click="invoice_btn_Click" Grid.Column="1" Height="20" />
        <Button Name="logout_btn" Content="Log Out" HorizontalAlignment="Left" Margin="0,110,0,0" VerticalAlignment="Top" Width="75" Click="logout_btn_Click" Grid.Column="1" Height="20" />
    </Grid>
</basic1:MyPage>
